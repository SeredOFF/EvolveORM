{
  "name": "seredoff/evolve-orm",
  "description": "Адаптивное ORM для проектов с динамично развивающейся доменной моделью",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/SeredOFF/EvolveORM",
  "keywords": [
    "hydrator",
    "performance",
    "array conversion",
    "object conversion",
    "serializer",
    "orm",
    "ddd",
    "domain"
  ],
  "authors": [
    {
      "name": "Daniil Seredov",
      "email": "dseredov@yandex.ru"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.2.0",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "ext-pdo": "*",
    "phpunit/phpunit": "^9.6",
    "phpstan/phpstan": "1.12.*",
    "rector/rector": "^1.1",
    "squizlabs/php_codesniffer": "3.*",
    "symfony/var-dumper": "^5.2",
    "ramsey/uuid": "^4.7",
    "fakerphp/faker": "^1.24"
  },
  "autoload": {
    "psr-4": {
      "EvolveORM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EvolveORM\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
